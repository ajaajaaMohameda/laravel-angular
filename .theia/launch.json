{
	"version": "0.2.0",
	"configurations": [

		{
			"name": "Listen for XDebug",
			"type": "php",
			"request": "launch",
            "port": 9009,
            "log": true,
            "pathMappings": {
                "/application": "/workspace/laravel-angular/chapter-04"
            }
		},
		{
			"name": "Launch currently open script",
			"type": "php",
			"request": "launch",
			"program": "${file}",
			"cwd": "${fileDirname}",
			"port": 9009
		}
	]
}
